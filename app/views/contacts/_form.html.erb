
<h1> Ainnnnnnnnnnnnnnnn </h1>

<%= form_for @contact do |f| %>
 <%= f.label :name %>
 <%= f.text_field :name %>

 
 <%= f.fields_for :address do |a| %>
   <%= a.label :street %> 
   <%= a.text_field :street %>

   <%= a.label :number %> 
   <%= a.text_field :number %>

   <%= a.label :neighborhood %> 
   <%= a.text_field :neighborhood %>
   
 <% end %>
 <%= f.submit 'Criar' %>
<% end %>



